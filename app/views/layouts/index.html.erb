<!-- app/views/index.html.erb -->
<!DOCTYPE html>
<html>
<head>
  <title>Home</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_pack_tag 'application' %>
  <style>
  .container {
  height: 100vh;
  width: 100vw;
  overflow-x: hidden;
  
}
.scrollable {
  overflow-y: scroll;
  margin: 0;
  padding: 0;
  min-height: 100vh;
  
}
.bottomnav {
  position: fixed;
  bottom: 0;
  height: 8vh;
  width: 100vw;
  background-color: #162633;
  padding: 0;
  margin: 0;
}
.header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 5vh;
  background-color: #066DAF;
  margin: 0;
  padding: 0;
}

.leftcontainer,
.rightcontainer {
  display: flex;
  align-items: center;
}

.transparent {
  background-color: transparent;
  border: none;
  cursor: pointer;
}

.transfer{
  background-color: transparent;
  border: none;
  cursor: pointer;
  z-index: 999;
}

.transparent:hover{
  cursor: pointer;
}

.transparent img {
  transition: opacity 0.2s;
}

.transparent:active img {
  opacity: 0.5;
  transition: none;
}
.transparent:active {
  opacity: 0.5;
  transition: none;
}
.bell,
.help {
  max-height: 3vh;
  margin-left: 2vw;
  margin-top: 1vh;
  align-self: center;
  padding: 0 2vw;
}
.eye{
  max-height: 2vh;
  margin-top: 1vh;
  align-self: center;

}

.logoutContainer {
  max-height: 3vh;
  margin-left: 0vw;
  align-self: center;
  margin-right: 5vw;
  border: 1.5px solid #ffffff;
  border-radius: 20px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.logoutText {
  margin: 0;
  padding: 1.5vw 4vw;
  color: #ffffff;
  text-align: center;
  font-size: 10px;
}
  
.alertbody {
    padding: 1vw 4vw;
    background-color: #066DAF;
}
  
.textheader {
    font-family: 'Lato';
    font-weight: 700;
    color: white;
    font-size: 22px;
    padding: 0;
    margin: 2vh 0vw 1vh 0vw;
  }
.textheaderftd1 {
  font-family: 'Lato';
  font-weight: 700;
  color: white;
  font-size: 30px;
  padding: 0;
  margin: 2vh 0vw 1vh 0vw;
}
.textheaderftd2 {
  font-family: 'Lato';
  font-weight: 700;
  color: #FFEB85;
  font-size: 24px;
  padding: 0;
  margin: 0 0vw 1vh 0vw;
}
  
.textbody {
    font-family: 'Lato';
    font-weight: normal;
    color: white;
    font-size: 15px;
    padding: 0;
    margin: 0;
}

.textbodyftd {
  font-family: 'Lato';
  font-weight: normal;
  color: white;
  font-size: 14px;
  padding: 0;
  margin: 0;
}
.homeheader{
  margin: 0;
  padding: 0;
  background-color: red;
}  
.alertbutton {
    background-color: #066DAF;
    display: flex;
    justify-content: flex-end;
}
  
.buttonContainer {
    background-color: #444444;
    border-radius: 20px;
    align-self: flex-end;
    margin-right: 5vw;
}
  

.buttonText {
    color: white;
    font-family: 'Lato';
    font-size: 13px;
    font-weight: bold;
    padding: 2.5vw 4vw;
    letter-spacing: 1px;
    margin: 2vw 0 5vw 0;
}
  
  .containertwo {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    margin-top: 3vw;
    height: 10vw;
    background-color: white;
    margin-left: 5vw;

  }
  
  .text {
    font-family: 'Lato';
    font-size: 20px;
    font-weight: bold;
    color: #444444;
    padding-right: 2vw;
  }
  
  .setting {
    object-fit: contain;
    max-height: 2vh;
  }
  
  .containerthree {
    background-color: white;
    overflow-x: auto;
    white-space: nowrap;
  }
  
  
  .iconcontainer {
    align-items: center;
    justify-content: center;
    width: 20vw;
  }
  
  .icons {
    width: 70%;
    height: 70%;
    object-fit: contain;
    padding-bottom: 1.5vw;
  }
  
.shortcut {
  flex-shrink: 1;
  font-family: 'Lato';
  font-size: 12px;
  font-weight: bold;
  text-align: center;
  color: #444444;
  padding: 0;
  margin: 0;
}
.containerfour {
  background-color: #EFEFEF;
  padding: 3vw 0vw;
  margin: 2vw 0 0 0;
}

.recenttransaction {
  width: 100vw;
  height: 7vh;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background-color: white;
  margin: 0;
  padding: 0;
}

.recenttransactiontext {
  font-family: 'Lato';
  font-size: 18px;
  color: #444444;
  font-weight: bold;
  margin:4vw 5vw;
}

.expand {
  object-fit: contain;
  padding: 0vw 5vw;
  width: 2.5vw;
}


</style>
</head>
<body>
<h1>hello</h1>
  <div class="top-bar">
    <div class="notification">24</div>
    <div class="travel">Travel: Off</div>
    <div class="help">?</div>
  </div>

  <div class="main-content">
    <div class="message">
      Stay vigilant against social media advertisement phishing, never click on suspicious links or download applications from unofficial app stores.
    </div>
    <div class="peek-balance">
      Tap and hold to PEEK BALANCE
    </div>
    <div class="buttons">
      <a href="<%= login_path %>" class="button">
        <img src="data:image/png;base64," alt="Log In">
        <div>Log In</div>
      </a>
      <div class="button">
        <img src="data:image/png;base64," alt="Digital Token">
        <div>Digital Token</div>
      </div>
      <div class="button">
        <img src="data:image/png;base64," alt="PayNow">
        <div>PayNow</div>
      </div>
      <div class="button">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQpib4j-rVdzAa7gJM78QYgLvXtRwZ2HMvVjA&s" alt="Overseas Transfer">
        <div>Overseas Transfer</div>
      </div>
      <div class="button">
        <img src="https://your-image-url.com/scan-pay-icon.png" alt="Scan & Pay">
        <div>Scan & Pay</div>
      </div>
      <div class="button">
        <img src="https://w7.pngwing.com/pngs/415/246/png-transparent-basic-ui-essential-interface-app-more-ui-essentials-icon-thumbnail.png" alt="More">
        <div>More</div>
      </div>
    </div>
  </div>

  <div class="promo">
    <img src="https://www.dbs.com.sg/iwov-resources/media/images/insurance/travel/travellershield-plus/dbs-promo-img.png" alt="Promo">
    <div>Make life wonderful with S$100 cashback</div>
  </div>

   <div className="container">
        <div className='scrollable'>
          <div className='homeheader'>
            <div className='header'>
              <div className='leftcontainer'>
                <button className='transparent' >
                  <img src='/assets/bellwhite.png' alt="bell" className='bell' />
                </button>
                <button className='transparent' >
                  <img  src='/assets/eye.png' className='eye' />
                </button>
              </div>

              <div className='rightcontainer'>
                <button className='transparent' >
                  <img  src='/assets/help.png' className='help' />
                </button>
                <button id='logoutButton' className='transparent'>
                  <div className='logoutContainer'>
                    <p className='logoutText'>LOG OUT</p>
                  </div>
                </button>
              </div>
            </div>
           
            <div>
              <div className='alertbody'>
                <p className='textheaderftd1'> </span>ALERT:</p>
                <p className='textheaderftd2'> YOU HAVE  FUND TRANSFER DISPUTE</p>
                <p className='textbodyftd'> It is encouraged to resolve the issue as soon as possible to prevent any implication with the 
  authorities.</p>
              </div>

              <div className='alertbutton' >
                <button className='transparent'>
                  <div className='buttonContainer'>
                      <p className='buttonText'>RESOLVE NOW</p>
                  </div>
                </button>      
              </div>
            </div>
            
            <div>
              <div className='alertbody'>
                <p className='textheader'> Make money work harder!</p>
                <p className='textbody'> Find out how you can put your cash flows to good use and achieve a sustainable financial future.</p>
              </div>
              
              <div className='alertbutton' >
                <button className='transparent'>
                  <div className='buttonContainer'>
                      <p className='buttonText'>LET'S TALK</p>
                  </div>
                </button>      
              </div>
            </div>
            
            
          </div>
          <div className='containertwo'>
            <p className='text'>Smart Shortcuts</p>
            <img  src='/assets/settings.png' className='setting' />
          </div>

          <div className='containerthree'>
            <div className='"scrollview'>  
              <button id = 'paynowbutton' className='transparent' >
                <div className='iconcontainer'>
        <img src="data:image/png;base64," alt="PayNow">
                  <p className='shortcut'>PayNow</p>
                </div>
              </button>
              
              <button id = 'transfermoney' className='transparent' >
                <div className='iconcontainer'>
        <img src="data:image/png;base64," alt="PayNow">
                  <p className='shortcut'>Transfer Money</p>
                </div>
              </button>
              
              <button id='scanpay' className='transparent' >
                <div className='iconcontainer'>
                  <img src='/assets/icons/scan&pay.png' className='icons' />
                  <p className='shortcut'>Scan & Pay</p>
                </div>
              </button>
              
              <button id = 'history' className='transparent' >
                <div className='iconcontainer'>
                  <img  src='/assets/icons/transaction_history.png' className='icons' />
                  <p className='shortcut'>Transaction</p>
                  <p className='shortcut'>History</p>
                </div>
              </button>
              
              <button className='transparent' >
                <div className='iconcontainer'>
                  <img  src='/assets/icons/local_transfer_limit.png' className='icons' />
                  <p className='shortcut'>Local Transfer</p>
                  <p className='shortcut'>Limit</p>
                </div>
              </button>
              
              <button className='transparent' >
                <div className='iconcontainer'>
                  <img  src='/assets/icons/livebetter.png' className='icons' />
                  <p className='shortcut'>Live Better</p>
                </div>
              </button>
              
              <button className='transparent' >
                <div className='iconcontainer'>
                  <img  src='/assets/icons/bill.png' className='icons' />
                  <p className='shortcut'>Bills</p>
                </div>
              </button>
              
              <button className='transparent' >
              <div className='iconcontainer'>
                <img  src='/assets/icons/setpeekbalance.png' className='icons' />
                <p className='shortcut'>Set Peek</p>
                <p className='shortcut'>Balance</p>

              </div>
              </button>
              
              <button className='transparent' >
                <div className='iconcontainer'>
                  <img  src='/assets/icons/scheduletransfer.png' className='icons' />
                  <p className='shortcut'>Transfers</p>
                  <p className='shortcut'>Scheduled</p>
                </div>
              </button>
            </div>
          </div>

          <div className="containerfour">
            <button id='transaction' className='transparent' >
              <div className="recenttransaction">
                <p className="recenttransactiontext">Recent Transactions</p>
                <img src='/assets/expand.png' className="expand"/>
              </div>
            </button>
          </div>

          <NavigationContainer>
            <TopNavigator></TopNavigator>
          </NavigationContainer>
        </div>
        <div className='bottomnav'>
          <BottomTabNavigator></BottomTabNavigator>
        </div>
    </div>
</body>
</html>
